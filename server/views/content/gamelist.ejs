<% include ../partials/header.ejs %>

    <!-- MAIN CONTENT -->
    <div class="container">
      <div class="row">
        <div class="col-md-offset-3 col-md-6">

          <br>
          <h3>Games List</h3>

          <table class="table table-striped table-hover table-bordered"> 
            <th>
              <td>Name</td>
              <td>Cost</td>
              <td>Rating</td>
              <td>Edit</td>
              <td>Delete</td>
            </th>
            <% for (let count = 0; count < games.length; count++) { %>
            <tr> 
              <td> <%= games[count].name %> </td>
              <td> <%= games[count].cost %> </td>
              <td> <%= games[count].rating %> </td>
              <td></td>
              <td></td>
            </tr>
            <% } %>
          </table>
        </div>
      </div>
    </div>

<% include ../partials/footer.ejs %>


